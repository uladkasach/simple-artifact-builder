{
  "name": "simple-artifact-builder",
  "version": "1.1.3",
  "description": "A tool to declaratively define best practices, maintainable evolve them, and scalably enforce them.",
  "author": "UladKasach @uladkasach",
  "license": "MIT",
  "repository": "uladkasach/simple-artifact-builder",
  "homepage": "https://github.com/uladkasach/simple-artifact-builder",
  "keywords": [
    "declarative",
    "best",
    "practice",
    "best-practice",
    "best-practices",
    "maintainable",
    "scalable",
    "repository",
    "linting",
    "fix"
  ],
  "bugs": "https://github.com/uladkasach/simple-artifact-builder/issues",
  "main": "dist/contract/index.js",
  "types": "dist/contract/index.d.ts",
  "bin": {
    "simple-artifact-builder": "./bin/run"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "oclif": {
    "commands": "./dist/contract/commands",
    "bin": "simple-artifact-builder",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "generate:cli-readme": "npm run build && oclif-dev readme",
    "format": "prettier --parser typescript --write 'src/**/*.ts' --config ./prettier.config.js",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "build:static-assets": "copyfiles -u 1 src/logic/contents/*.js dist/",
    "build": "rm -rf ./dist && npm run build:ts && npm run build:static-assets",
    "test:types": "tsc --noEmit",
    "test:format": "prettier --parser typescript --check 'src/**/*.ts' --config ./prettier.config.js",
    "test:lint": "tslint -c tslint.json -p tsconfig.json",
    "test:unit": "FORCE_COLOR=1 TZ=UTC jest --forceExit --coverage --verbose",
    "test:integration": "FORCE_COLOR=1 TZ=UTC jest -c ./jest.integration.config.js --forceExit --coverage --verbose --runInBand",
    "test": "npm run test:types && npm run test:format && npm run test:lint && npm run test:unit && npm run test:integration",
    "preversion": "npm run test && npm run build",
    "postversion": "git push origin HEAD --tags --no-verify",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/archiver": "5.1.1",
    "@types/flat": "5.0.2",
    "@types/jest": "25.1.3",
    "@types/joi": "17.2.3",
    "@types/lodash.uniqby": "4.7.6",
    "@types/pg": "8.6.1",
    "@types/shelljs": "0.8.9",
    "@types/uuid": "3.4.4",
    "@types/yaml": "1.0.2",
    "@types/yesql": "3.2.1",
    "copyfiles": "2.4.1",
    "core-js": "3.6.5",
    "husky": "1.3.1",
    "jest": "27.1.0",
    "node-stream-zip": "1.15.0",
    "pg": "8.6.0",
    "prettier": "1.19.1",
    "simple-lambda-client": "2.1.3",
    "ts-jest": "27.0.5",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-consistent-codestyle": "1.13.3",
    "typescript": "4.3.2",
    "uuid": "3.3.2",
    "yesql": "3.2.2"
  },
  "dependencies": {
    "@oclif/command": "1.7.0",
    "@oclif/config": "1.16.0",
    "@oclif/plugin-help": "3.1.0",
    "@vercel/nft": "0.17.0",
    "archiver": "5.3.0",
    "chalk": "2.4.2",
    "cross-path-sort": "1.0.0",
    "diff": "4.0.1",
    "domain-objects": "0.7.1",
    "domain-objects-metadata": "0.4.1",
    "expect": "27.1.0",
    "fast-glob": "3.2.2",
    "find-nearest-package-json": "2.0.1",
    "flat": "5.0.2",
    "indent-string": "4.0.0",
    "jest-diff": "27.1.0",
    "joi": "17.4.0",
    "lodash.omit": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "oclif": "1.16.1",
    "pretty-bytes": "5.6.0",
    "shelljs": "0.8.4",
    "simple-leveled-log-methods": "0.1.4",
    "simple-sha256": "1.0.0",
    "simple-type-guards": "0.2.0",
    "ts-node": "8.6.2",
    "yaml": "1.6.0"
  }
}
